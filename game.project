[project]
title = DefBlend
dependencies#0 = https://github.com/rgrams/rendercam/archive/master.zip

[library]
include_dirs = defblend

[input]
game_binding = /example/main.input_bindingc

[bootstrap]
main_collection = /example/main.collectionc
render = /rendercam/rendercam.renderc

[script]
shared_state = 1

[physics]
debug = 1
ray_cast_limit_2d = 1024

[display]
variable_dt = 1
width = 800
height = 600

[sprite]
max_count = 2056

